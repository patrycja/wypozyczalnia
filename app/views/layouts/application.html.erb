<!DOCTYPE html>
<html>
<head>
  <title>Wypozyczalnia</title>
  <%= stylesheet_link_tag "style" %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id='container'>
<%= yield %><br />
<% if flash[:notice] %>
    <div id="flash_notice"><%= flash[:notice] %></div>
<% end %>

<% if flash[:error] %>
    <div id="flash_error"><%= flash[:error] %></div>
<% end %>
</div><!--container-->
</body>
</html>
